<h1> Sign In! </h1>

<p>
  No account yet?  <%= link_to "Sign Up", signup_path%>
</p>

<%= form_with(url: session_path) do |f|%>

<%= f.label :email, autofocus: true%>
<%= f.email_field :email, required: true%>

<%= f.label :password%>
<%= f.password_field :password, required: true%>

<%= f.submit "Sign In"%>

<%end%>
